<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/parking_red1"
        android:layout_width="300dp"
        android:layout_height="170dp"
        android:layout_marginBottom="512dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/parking_red"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/parking_green2"
        android:layout_width="300dp"
        android:layout_height="170dp"
        android:layout_marginBottom="340dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/parking_green"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="300dp"
        android:layout_height="160dp"
        android:layout_marginBottom="164dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mypraking"
        tools:ignore="MissingConstraints" />


    <RelativeLayout
        android:layout_width="292dp"
        android:layout_height="154dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.712"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/ParkingNum"
            android:layout_width="187dp"
            android:layout_height="81dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="50dp"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="-10dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="100dp"
            android:fontFamily="@font/montserrat_extrabold"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-50dp"
            android:text="No."
            android:textSize="50dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView1"
            app:layout_constraintEnd_toEndOf="@+id/imageView1"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/imageView1"
            app:layout_constraintTop_toTopOf="@+id/imageView1"
            app:layout_constraintVertical_bias="0.001"
            tools:ignore="MissingConstraints,NotSibling" />

        <TextView
            android:id="@+id/carnum"
            android:layout_width="109dp"
            android:layout_height="39dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="92dp"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="91dp"
            android:layout_marginBottom="43dp"
            android:text="차량 번호"
            android:textSize="24dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/home_id"
        android:layout_width="wrap_content"
        android:layout_height="84dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#5484FF"
        android:clipToOutline="true"
        android:elevation="4dp"
        android:onClick="HomeClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="183dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="183dp"
            android:importantForAccessibility="no"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/home" />
    </RelativeLayout>

    <TextView
        android:id="@+id/P01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:rotation="270"
        android:text="P01"
        app:layout_constraintBottom_toBottomOf="@+id/parking_red1"
        app:layout_constraintEnd_toEndOf="@+id/parking_red1"
        app:layout_constraintHorizontal_bias="0.01"
        app:layout_constraintStart_toStartOf="@+id/parking_red1"
        app:layout_constraintTop_toTopOf="@+id/parking_red1"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/P02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:rotation="270"
        android:text="P02"
        app:layout_constraintBottom_toBottomOf="@+id/parking_green2"
        app:layout_constraintEnd_toEndOf="@+id/parking_green2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/parking_green2"
        app:layout_constraintTop_toBottomOf="@+id/parking_red1"
        app:layout_constraintVertical_bias="0.5
"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/return_car"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:text="반납하기"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/home_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.838"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>