<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="289dp"
        android:layout_height="159dp"
        android:layout_marginBottom="164dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/parking_green2"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="@+id/parking_green2"
        app:layout_constraintTop_toBottomOf="@+id/parking_green2"
        app:layout_constraintVertical_bias="0.406"
        app:srcCompat="@drawable/mypraking"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/parking_green1"
        android:layout_width="300dp"
        android:layout_height="170dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/parking_green2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/background_gradient"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/parking_green2"
        android:layout_width="300dp"
        android:layout_height="170dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/parking_green1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/parking_green1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.367"
        app:srcCompat="@drawable/background_gradient"
        tools:ignore="MissingConstraints" />

    <RelativeLayout
        android:id="@+id/home_id"
        android:layout_width="407dp"
        android:layout_height="84dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#5484FF"
        android:clipToOutline="true"
        android:elevation="4dp"
        android:onClick="HomeClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="74dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="185dp"
            android:layout_marginTop="-6dp"
            android:layout_marginEnd="172dp"
            android:importantForAccessibility="no"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/home" />

    </RelativeLayout>

    <TextView
        android:id="@+id/P01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:rotation="270"
        android:text="P01"
        app:layout_constraintBottom_toBottomOf="@+id/parking_green1"
        app:layout_constraintEnd_toEndOf="@+id/parking_green1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/parking_green1"
        app:layout_constraintTop_toTopOf="@+id/parking_green1"
        app:layout_constraintVertical_bias="0.48"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/P02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:rotation="270"
        android:text="P02"
        app:layout_constraintBottom_toBottomOf="@+id/parking_green2"
        app:layout_constraintEnd_toEndOf="@+id/parking_green2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/parking_green2"
        app:layout_constraintTop_toBottomOf="@+id/parking_green1"
        app:layout_constraintVertical_bias="0.493"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/return_car"
        android:layout_width="104dp"
        android:layout_height="50dp"
        android:background="#EBEBEB"
        android:fontFamily="@font/inter"
        android:gravity="center"
        android:onClick="onReturnButtonClicked"
        android:text="반납하기"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/home_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.383"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.838"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/refresh_button"
        android:layout_width="48dp"
        android:layout_height="42dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.694"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.837"
        app:srcCompat="@drawable/refresh"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/ParkingNum"
        android:layout_width="187dp"
        android:layout_height="81dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="56dp"
        android:fontFamily="@font/montserrat_extrabold"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="-50dp"
        android:text="No."
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView1"
        app:layout_constraintEnd_toEndOf="@+id/imageView1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView1"
        app:layout_constraintTop_toTopOf="@+id/imageView1"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints,NotSibling" />

