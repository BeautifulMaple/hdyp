<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:ignore="MissingClass">

    <!-- Auto layout, variables, and unit scale are not yet supported -->

    <RelativeLayout
        android:id="@+id/h_d_y_p"
        android:layout_width="409dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#5484FF"
        android:clipToOutline="true"
        android:elevation="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/h_d_y_p_txt"
            android:layout_width="230dp"
            android:layout_height="42dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="89dp"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="19dp"
            android:fontFamily="@font/inter_x"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-4sp"
            android:text="@string/h_d_y_p"
            android:textAppearance="@style/h_d_y_p" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_1"
        android:layout_width="281dp"
        android:layout_height="152dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginBottom="464dp"
        android:background="#5484FF"
        android:clipToOutline="true"
        android:elevation="4dp"
        android:onClick="btnClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="28dp"
            android:layout_height="36.67dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="62dp"
            android:layout_marginBottom="54dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/mappin" />

        <TextView
            android:id="@+id/parkingtext"
            android:layout_width="186dp"
            android:layout_height="64dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="46dp"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-8sp"
            android:text="@string/parkingtext1"
            android:textAppearance="@style/parking_text1" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_2"
        android:layout_width="281dp"
        android:layout_height="152dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginBottom="140dp"
        android:background="#5484FF"
        android:clipToOutline="true"
        android:elevation="4dp"
        android:onClick="btnClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_1"
        app:layout_constraintVertical_bias="0.447">


        <TextView
            android:id="@+id/parkingtext2"
            android:layout_width="209dp"
            android:layout_height="60dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="36dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="46dp"
            android:layout_marginEnd="36dp"
            android:layout_marginBottom="46dp"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="-8sp"
            android:text="ID 입력"
            android:textAppearance="@style/parking_text1" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/some_id"
        android:layout_width="407dp"
        android:layout_height="84dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#5484FF"
        android:clipToOutline="true"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" >

        <TextView
            android:id="@+id/sensorDataView"
            android:layout_width="107dp"
            android:layout_height="58dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="155dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="155dp"
            android:layout_marginBottom="5dp"
            android:text="@string/addAd"
            android:textSize="50sp" />
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
